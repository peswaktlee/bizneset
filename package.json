    "@types/react": "18.3.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "@types/react-dom": "18.3.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-0dec889-20241115",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "autoprefixer": "^10.0.1",
    "knip": "^5.38.3",
