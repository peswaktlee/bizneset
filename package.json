    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-firebase-hooks": "^5.1.1",
    "react-infinite-scroll-hook": "^4.1.1",
    "tailwind-merge": "^2.5.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
